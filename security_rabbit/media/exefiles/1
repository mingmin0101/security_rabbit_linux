import subprocess


#args = ["wine", "sigcheck.exe", "-i", "-l", "-nobanner", "/mnt/security_rabbit/security_rabbit/media/file_upload/javaw.exe"]
args = ["wime", "--help"]
pipe = subprocess.Popen(args, stdout=subprocess.PIPE, shell=True)
sigcheck_output = pipe.communicate()[0]

print(sigcheck_output.decode('utf-8','replace'))
